
; Player information
[Info]
name         = "ShadowedNeville"   ; Name of character
displayname  = "Shadowed Neville"  ; Name of character to display
versiondate  = 3/30/2020          ; Version date of character (MM,DD,YYYY or X.XX)
mugenversion = 1.1          ; Version of M.U.G.E.N. character works on (X.XX)
author       = "Jackachu"   ; Character author name
pal.defaults = 1,2,3,4,5,6   ; Default palettes in order of preference (up to 4, starts from 1)
localcoord   = 320,240       ; Local coordinate space (width, height)

; Files for the player
[Files]
cmd      = ShadowedNeville.cmd          ; Command set
cns      = ShadowedNeville.cns          ; Constants
st       = ShadowedNeville.cns          ; States
stcommon = common1.cns       ; Common states (from data or motif)
sprite   = ShadowedNeville.sff          ; Sprite
anim     = ShadowedNeville.air          ; Animation
sound    = ShadowedNeville.snd          ; Sound
ai       = ShadowedNeville.ai           ; AI hints data (not used)

; Maps character selection buttons to palette numbers.
[Palette Keymap]
x  = 1 ;Press button X to select palette 1, etc.
y  = 2
z  = 3
a  = 4
b  = 5
c  = 6

; Arcade mode
[Arcade]
;intro.storyboard = intro.def
;ending.storyboard = ending.def
